{
  "private": true,
  "name": "blotout-website",
  "scripts": {
    "build": "remix build",
    "build:sass": "sass --no-source-map scss:app/styles",
    "dev": "remix dev",
    "start": "vercel dev",
    "main": "concurrently \"vercel dev\" \"remix dev\"",
    "sass": "sass --watch --embed-source-map scss:app/styles",
    "lint": "prettier --check .",
    "postinstall": "remix setup"
  },
  "dependencies": {
    "@blotoutio/sdk-core": "^0.9.1",
    "@carbon/icons-react": "^10.41.0",
    "@remix-run/react": "^0.18.0",
    "@remix-run/vercel": "^0.18.0",
    "@vercel/node": "^1.8.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0",
    "remix": "^0.18.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.18.0",
    "concurrently": "^6.2.0",
    "prettier": "^2.3.2",
    "sass": "^1.37.0",
    "vercel": "^21.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
